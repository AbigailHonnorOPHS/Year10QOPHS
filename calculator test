num1=int(input("Enter first number>>> "))
op=input("Enter type of calculation e.g. + >>> ")
num2=int(input("Enter second number>>> "))


if op == "+":
  print(num1 + num2)
elif op == "-":
  print(num1 - num2)
elif op == "/":
  print(num1/num2)
elif op == "*":
  print(num1*num2)
else:
  print("mathmatical error")
